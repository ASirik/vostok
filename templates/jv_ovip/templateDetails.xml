	<params>

		<param name="jv_font" type="list" default="3" label="Font size" description="Select Font size for your site">
			<option value="1">1</option>
			<option value="2">2</option>
			<option value="3">3</option>
			<option value="4">4</option>
			<option value="5">5</option>
			<option value="6">6</option>
		</param>
		<param name="menutype" type="menu" default="mainmenu" label="Menu Type" description="Menu to show on main navigation" />
		<param name="jv_menustyle" type="list" default="moo" label="Menu Module" description="MENU'S TYPE">
		  <option value="none">Disable</option>
		  <option value="moo">Moo Menu</option>
		  <option value="css">CSS Menu</option>
		</param>
		<param name="showtools" type="radio" default="1" label="Show Tools of template" description="">
			<option value="1">Show</option>
			<option value="0">Hide</option>
		</param>
		<param name="jv_color" type="list" default="neon" label="Color Variation" description="Color variation to use">
			<option value="neon">Neon</option>
			<option value="laser">Laser</option>
			<option value="fable">Fable</option>
		</param>
		<param name="spacer@" type="spacer" />
		<param name="gzip" type="radio" default="0" label="Gzip Compression" description="Do you want to enable gzip compression for Javascript and Css (Some host dont have gzip module enabled, If see that your site is not loading css and js please disable this parameter.">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
	</params>

